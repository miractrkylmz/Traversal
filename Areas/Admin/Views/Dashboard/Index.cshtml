    <script src="/Traversal-AdminTemasi/assets/vendor/libs/apex-charts/apexcharts.js"></script>

    <!-- Main JS -->
    <script src="/Traversal-AdminTemasi/assets/js/main.js"></script>

    <!-- Page JS -->
    <script src="/Traversal-AdminTemasi/assets/js/dashboards-analytics.js"></script>

    <!-- Place this tag in your head or just before your close body tag. -->
    <script async defer src="https://buttons.github.io/buttons.js"></script>
