
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en"
      class="light-style layout-menu-fixed"
      dir="ltr"
      data-theme="theme-default"
      data-assets-path="/Traversal-AdminTemasi/assets/"
      data-template="vertical-menu-template-free">

@await Html.PartialAsync("/Views/AdminLayoutPartial/_HeaderPartial.cshtml")

<body>
    <div class="layout-wrapper layout-content-navbar">
        <div class="layout-container">

            @await Html.PartialAsync("/Views/AdminLayoutPartial/_LeftNavbarPartial.cshtml")

            <div class="layout-page">

                @await Html.PartialAsync("/Views/AdminLayoutPartial/_TopNavbarPartial.cshtml")

                <div class="content-wrapper">
                    <div class="container-xxl flex-grow-1 container-p-y">       
                        
                        @RenderBody()
                    </div>

                    @await Html.PartialAsync("/Views/AdminLayoutPartial/_FooterPartial.cshtml")

                    <div class="content-backdrop fade"></div>
                </div>
            </div>
        </div>

        <div class="layout-overlay layout-menu-toggle"></div>
    </div>

    @await Html.PartialAsync("/Views/AdminLayoutPartial/_ScriptsPartial.cshtml")

</body>
</html>
